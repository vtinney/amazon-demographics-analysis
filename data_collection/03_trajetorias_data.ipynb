    "## Step 5: Extract Key Variables\n",
    "\n",
    "Identify and extract the most important variables for our PM2.5 analysis."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Define key variables of interest for PM2.5 analysis\n",
    "cat(\"üéØ Identifying key variables for PM2.5 analysis...\\n\")\n",
    "\n",
    "key_variables <- list(\n",
    "  socioeconomic = c(\n",
    "    \"mpi_rural\", \"mpi_urban\", \"mpi_general\",  # Multidimensional Poverty Index\n",
    "    \"poverty_incidence\", \"poverty_intensity\",   # Poverty measures\n",
    "    \"income\", \"education\", \"employment\"          # Socioeconomic indicators\n",
    "  ),\n",
    "  epidemiological = c(\n",
    "    \"malaria\", \"dengue\", \"leishmaniasis\",        # Vector-borne diseases\n",
    "    \"respiratory\", \"diarrhea\",                   # Health indicators\n",
    "    \"health_access\", \"hospital_density\"          # Health system access\n",
    "  ),\n",
    "  environmental = c(\n",
    "    \"deforestation\", \"forest_cover\",             # Forest indicators\n",
    "    \"agriculture\", \"pasture\", \"urban_area\",      # Land use\n",
    "    \"precipitation\", \"temperature\",              # Climate\n",
    "    \"road_density\", \"mining\"                     # Infrastructure/extractive\n",
    "  ),\n",
    "  population = c(\n",
    "    \"population_density\", \"urban_population\",    # Population distribution\n",
    "    \"migration\", \"age_structure\"                 # Demographic characteristics\n",
    "  )\n",
    ")\n",
    "\n",
    "# Function to extract available key variables\n",
    "extract_key_variables <- function(data, component_name, key_vars) {\n",
    "  if (is.null(data)) return(NULL)\n",
    "  \n",
    "  cat(\"\\nüîç Extracting key variables from\", component_name, \"...\\n\")\n",
    "  \n",
    "  # Get column names (case insensitive matching)\n",
    "  available_cols <- names(data)\n",
    "  \n",
    "  # Find variables that match our key variables (partial matching)\n",
    "  found_vars <- c()\n",
    "  for (key_var in key_vars) {\n",
    "    # Look for columns containing the key variable name\n",
    "    matches <- grep(key_var, available_cols, ignore.case = TRUE, value = TRUE)\n",
    "    if (length(matches) > 0) {\n",
    "      found_vars <- c(found_vars, matches[1])  # Take first match\n",
    "      cat(\"   ‚úÖ Found:\", key_var, \"‚Üí\", matches[1], \"\\n\")\n",
    "    }\n",
    "  }\n",
    "  \n",
    "  # Always include essential columns\n",
    "  essential_cols <- c(\"code_muni\", \"year\", \"abbrev_state\")\n",
    "  essential_available <- essential_cols[essential_cols %in% available_cols]\n",
    "  \n",
    "  # Select found variables plus essential columns\n",
    "  selected_cols <- unique(c(essential_available, found_vars))\n",
    "  \n",
    "  if (length(found_vars) > 0) {\n",
    "    extracted_data <- data %>% select(all_of(selected_cols))\n",
    "    cat(\"üìã Extracted\", length(found_vars), \"key variables +\", length(essential_available), \"essential columns\\n\")\n",
    "    return(extracted_data)\n",
    "  } else {\n",
    "    cat(\"‚ö†Ô∏è No matching key variables found\\n\")\n",
    "    return(NULL)\n",
    "  }\n",
    "}\n",
    "\n",
    "# Extract key variables from each component\n",
    "key_indicators <- list()\n",
    "\n",
    "for (component in names(processed_trajetorias)) {\n",
    "  if (component %in% names(key_variables)) {\n",
    "    extracted <- extract_key_variables(\n",
    "      processed_trajetorias[[component]],\n",
    "      component,\n",
    "      key_variables[[component]]\n",
    "    )\n",
    "    \n",
    "    if (!is.null(extracted)) {\n",
    "      key_indicators[[component]] <- extracted\n",
    "      \n",
    "      # Save key indicators\n",
    "      filename <- paste0(\"data/processed/key_indicators_\", component, \".csv\")\n",
    "      save_data_with_metadata(\n",
    "        extracted,\n",
    "        filename,\n",
    "        paste(\"Key\", component, \"indicators for PM2.5 analysis\")\n",
    "      )\n",
    "    }\n",
    "  }\n",
    "}"
   ]
