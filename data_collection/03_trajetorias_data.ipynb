    "## Step 1: Load Configuration and Libraries"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Load essential libraries\n",
    "library(httr)\n",
    "library(jsonlite)\n",
    "library(readr)\n",
    "library(dplyr)\n",
    "library(ggplot2)\n",
    "library(DT)\n",
    "library(knitr)\n",
    "\n",
    "# Load project configuration if available\n",
    "if (file.exists(\"data/project_config.rds\")) {\n",
    "  config <- readRDS(\"data/project_config.rds\")\n",
    "  amazon_states <- config$amazon_states\n",
    "  cat(\"‚úÖ Configuration loaded successfully\\n\")\n",
    "} else {\n",
    "  # Fallback - define Amazon states\n",
    "  amazon_states <- c(\"AC\", \"AP\", \"AM\", \"MA\", \"MT\", \"PA\", \"RO\", \"RR\", \"TO\")\n",
    "  cat(\"‚ö†Ô∏è Using fallback configuration\\n\")\n",
    "}\n",
    "\n",
    "# Helper functions\n",
    "standardize_muni_code <- function(code) {\n",
    "  sprintf(\"%07d\", as.numeric(code))\n",
    "}\n",
    "\n",
    "save_data_with_metadata <- function(data, filename, description = \"\") {\n",
    "  dir.create(dirname(filename), recursive = TRUE, showWarnings = FALSE)\n",
    "  write_csv(data, filename)\n",
    "  \n",
    "  metadata <- list(\n",
    "    filename = basename(filename),\n",
    "    created = Sys.time(),\n",
    "    rows = nrow(data),\n",
    "    columns = ncol(data),\n",
    "    description = description\n",
    "  )\n",
    "  \n",
    "  metadata_file <- paste0(tools::file_path_sans_ext(filename), \"_metadata.json\")\n",
    "  write(jsonlite::toJSON(metadata, pretty = TRUE), metadata_file)\n",
    "  cat(\"üíæ Saved:\", filename, \"(\", nrow(data), \"rows)\\n\")\n",
    "}\n",
    "\n",
    "cat(\"üìö Libraries loaded and helper functions defined\\n\")"
   ]
