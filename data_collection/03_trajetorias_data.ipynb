    "# Function to load and explore Trajetorias data\n",
    "explore_trajetorias_component <- function(component_name) {\n",
    "  filename <- paste0(\"data/raw/trajetorias/\", component_name, \"_indicators.csv\")\n",
    "  \n",
    "  if (!file.exists(filename)) {\n",
    "    cat(\"‚ùå\", component_name, \"file not found\\n\")\n",
    "    return(NULL)\n",
    "  }\n",
    "  \n",
    "  cat(\"\\n=== \", toupper(component_name), \" COMPONENT ===\\n\")\n",
    "  \n",
    "  # Load data\n",
    "  tryCatch({\n",
    "    data <- read_csv(filename, show_col_types = FALSE)\n",
    "    \n",
    "    # Basic information\n",
    "    cat(\"üìä Dimensions:\", nrow(data), \"rows √ó\", ncol(data), \"columns\\n\")\n",
    "    \n",
    "    # Check for key columns\n",
    "    if (\"code_muni\" %in% names(data)) {\n",
    "      cat(\"üèòÔ∏è Municipalities:\", length(unique(data$code_muni)), \"\\n\")\n",
    "    }\n",
    "    \n",
    "    if (\"year\" %in% names(data)) {\n",
    "      years <- sort(unique(data$year))\n",
    "      cat(\"üìÖ Years available:\", paste(years, collapse = \", \"), \"\\n\")\n",
    "    }\n",
    "    \n",
    "    # Show column names (first 10)\n",
    "    cat(\"üìã Columns (first 10):\\n\")\n",
    "    col_preview <- head(names(data), 10)\n",
    "    for (col in col_preview) {\n",
    "      cat(\"   -\", col, \"\\n\")\n",
    "    }\n",
    "    \n",
    "    if (ncol(data) > 10) {\n",
    "      cat(\"   ... and\", ncol(data) - 10, \"more columns\\n\")\n",
    "    }\n",
    "    \n",
    "    # Check for Amazon municipalities if code_muni exists\n",
    "    if (\"code_muni\" %in% names(data)) {\n",
    "      # Standardize codes\n",
    "      data$code_muni <- standardize_muni_code(data$code_muni)\n",
    "      \n",
    "      # Check coverage - we'll need to merge with census data to get state info\n",
    "      cat(\"üîç Municipal codes standardized\\n\")\n",
    "    }\n",
    "    \n",
    "    return(data)\n",
    "    \n",
    "  }, error = function(e) {\n",
    "    cat(\"‚ùå Error loading\", component_name, \":\", e$message, \"\\n\")\n",
