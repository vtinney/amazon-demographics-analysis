   "source": [
    "# Function to download files from Zenodo\n",
    "download_trajetorias_file <- function(url, filename) {\n",
    "  cat(\"‚¨áÔ∏è Downloading:\", filename, \"...\")\n",
    "  \n",
    "  tryCatch({\n",
    "    # Create directory if it doesn't exist\n",
    "    dir.create(dirname(filename), recursive = TRUE, showWarnings = FALSE)\n",
    "    \n",
    "    # Download file\n",
    "    response <- GET(url, timeout(300))  # 5 minute timeout\n",
    "    \n",
    "    if (status_code(response) == 200) {\n",
    "      # Save file\n",
    "      writeBin(content(response, \"raw\"), filename)\n",
    "      \n",
    "      # Check file size\n",
    "      file_size <- file.info(filename)$size\n",
    "      cat(\" ‚úÖ Success (\", round(file_size/1024/1024, 1), \"MB)\\n\")\n",
    "      return(TRUE)\n",
    "    } else {\n",
    "      cat(\" ‚ùå Failed (HTTP\", status_code(response), \")\\n\")\n",
    "      return(FALSE)\n",
    "    }\n",
    "    \n",
    "  }, error = function(e) {\n",
    "    cat(\" ‚ùå Error:\", e$message, \"\\n\")\n",
    "    return(FALSE)\n",
    "  })\n",
    "}\n",
    "\n",
    "# Download main data files\n",
    "cat(\"\\nüì• Downloading main dataset files...\\n\")\n",
    "download_success <- list()\n",
    "\n",
    "for (name in names(trajetorias_urls)) {\n",
    "  filename <- paste0(\"data/raw/trajetorias/\", name, \"_indicators.csv\")\n",
    "  download_success[[name]] <- download_trajetorias_file(trajetorias_urls[[name]], filename)\n",
    "}\n",
    "\n",
    "# Download metadata files\n",
    "cat(\"\\nüìã Downloading metadata files...\\n\")\n",
    "for (name in names(metadata_urls)) {\n",
    "  filename <- paste0(\"data/raw/trajetorias/\", name, \".csv\")\n",
    "  download_trajetorias_file(metadata_urls[[name]], filename)\n",
    "}\n",
    "\n",
    "# Summary of downloads\n",
    "successful_downloads <- sum(unlist(download_success))\n",
    "total_downloads <- length(download_success)\n",
    "\n",
    "cat(\"\\nüìä Download Summary:\\n\")\n",
    "cat(\"   Successful:\", successful_downloads, \"/\", total_downloads, \"main files\\n\")\n",
    "\n",
    "if (successful_downloads == total_downloads) {\n",
    "  cat(\"‚úÖ All downloads completed successfully!\\n\")\n",
    "} else {\n",
    "  cat(\"‚ö†Ô∏è Some downloads failed. Proceeding with available data...\\n\")\n",
    "}"
   ]
