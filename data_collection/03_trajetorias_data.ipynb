   "source": [
    "# Define Zenodo URLs for Trajetorias dataset\n",
    "cat(\"üåê Preparing to download Trajetorias dataset...\\n\")\n",
    "\n",
    "base_url <- \"https://zenodo.org/records/7098053/files/\"\n",
    "\n",
    "# Main data files\n",
    "trajetorias_urls <- list(\n",
    "  population = paste0(base_url, \"TRAJETORIAS_DATASET_Population_indicators.csv\"),\n",
    "  socioeconomic = paste0(base_url, \"TRAJETORIAS_DATASET_Socio-Economic_dimension-indicators.csv\"),\n",
    "  epidemiological = paste0(base_url, \"TRAJETORIAS_DATASET_Epidemiological_dimension_indicators.csv\"),\n",
    "  environmental = paste0(base_url, \"TRAJETORIAS_DATASET_Environmental_dimension_indicators.csv\")\n",
    ")\n",
    "\n",
    "# Metadata files\n",
    "metadata_urls <- list(\n",
    "  population_meta = paste0(base_url, \"TRAJETORIAS_DATASET_Population_indicators_METADATA.csv\"),\n",
    "  socioeconomic_meta = paste0(base_url, \"TRAJETORIAS_DATASET_Socio-Economic_dimension-indicators_METADATA.csv\"),\n",
    "  epidemiological_meta = paste0(base_url, \"TRAJETORIAS_DATASET_Epidemiological_dimension_indicators_METADATA.csv\"),\n",
    "  environmental_meta = paste0(base_url, \"TRAJETORIAS_DATASET_Environmental_dimension_indicators_METADATA.csv\")\n",
    ")\n",
    "\n",
    "cat(\"üìã Dataset components identified:\\n\")\n",
    "cat(\"   - Population indicators\\n\")\n",
    "cat(\"   - Socio-economic indicators\\n\")\n",
    "cat(\"   - Epidemiological indicators\\n\")\n",
    "cat(\"   - Environmental indicators\\n\")\n",
    "cat(\"   - Metadata for each component\\n\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
