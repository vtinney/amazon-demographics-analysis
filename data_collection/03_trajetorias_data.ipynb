   "source": [
    "## Step 4: Filter for Amazon Region and Recent Years\n",
    "\n",
    "Extract the most relevant data for our Amazon analysis."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Load census data to get state mappings\n",
    "cat(\"üó∫Ô∏è Loading municipality-state mappings...\\n\")\n",
    "\n",
    "# Try to load census data for state mapping\n",
    "state_mapping <- NULL\n",
    "if (file.exists(\"data/raw/census/amazon_population_2022.csv\")) {\n",
    "  census_data <- read_csv(\"data/raw/census/amazon_population_2022.csv\", show_col_types = FALSE)\n",
    "  state_mapping <- census_data %>% \n",
    "    select(code_muni, abbrev_state, name_muni) %>%\n",
    "    mutate(code_muni = standardize_muni_code(code_muni))\n",
    "  \n",
    "  cat(\"‚úÖ Loaded state mapping for\", nrow(state_mapping), \"Amazon municipalities\\n\")\n",
    "} else {\n",
    "  cat(\"‚ö†Ô∏è Census data not available. Will process all municipalities.\\n\")\n",
    "}\n",
    "\n",
    "# Function to process and filter Trajetorias component\n",
    "process_trajetorias_component <- function(data, component_name, state_mapping = NULL) {\n",
    "  if (is.null(data)) {\n",
    "    cat(\"‚ö†Ô∏è No data available for\", component_name, \"\\n\")\n",
    "    return(NULL)\n",
    "  }\n",
    "  \n",
    "  cat(\"\\nüîÑ Processing\", component_name, \"component...\\n\")\n",
    "  \n",
    "  # Standardize municipal codes\n",
    "  if (\"code_muni\" %in% names(data)) {\n",
    "    data$code_muni <- standardize_muni_code(data$code_muni)\n",
    "  }\n",
    "  \n",
    "  # Filter for Amazon municipalities if we have state mapping\n",
    "  if (!is.null(state_mapping) && \"code_muni\" %in% names(data)) {\n",
    "    amazon_munis <- state_mapping$code_muni\n",
    "    data_amazon <- data %>% filter(code_muni %in% amazon_munis)\n",
    "    \n",
    "    cat(\"üåø Filtered to\", nrow(data_amazon), \"Amazon municipality records\\n\")\n",
    "    data <- data_amazon\n",
    "  }\n",
    "  \n",
    "  # Get most recent year if multiple years available\n",
    "  if (\"year\" %in% names(data)) {\n",
    "    latest_year <- max(data$year, na.rm = TRUE)\n",
    "    data_recent <- data %>% filter(year == latest_year)\n",
    "    \n",
    "    cat(\"üìÖ Using most recent year:\", latest_year, \"(\", nrow(data_recent), \"records)\\n\")\n",
    "    data <- data_recent\n",
    "  }\n",
    "  \n",
    "  # Add state information if available\n",
    "  if (!is.null(state_mapping) && \"code_muni\" %in% names(data)) {\n",
    "    data <- data %>% \n",
    "      left_join(state_mapping %>% select(code_muni, abbrev_state), \n",
    "                by = \"code_muni\")\n",
    "    cat(\"üèõÔ∏è Added state information\\n\")\n",
    "  }\n",
    "  \n",
    "  return(data)\n",
    "}\n",
    "\n",
    "# Process each component\n",
    "processed_trajetorias <- list()\n",
    "\n",
    "for (component in names(trajetorias_data)) {\n",
    "  processed_data <- process_trajetorias_component(\n",
    "    trajetorias_data[[component]], \n",
    "    component, \n",
    "    state_mapping\n",
    "  )\n",
    "  \n",
    "  if (!is.null(processed_data)) {\n",
    "    processed_trajetorias[[component]] <- processed_data\n",
    "    \n",
    "    # Save processed data\n",
    "    filename <- paste0(\"data/processed/trajetorias_\", component, \"_amazon.csv\")\n",
    "    save_data_with_metadata(\n",
    "      processed_data, \n",
    "      filename,\n",
    "      paste(\"Processed\", component, \"indicators from Trajetorias dataset for Amazon region\")\n",
    "    )\n",
    "  }\n",
    "}"
   ]
