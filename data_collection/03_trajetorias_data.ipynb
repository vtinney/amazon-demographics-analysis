   "source": [
    "# Create summary visualizations\n",
    "cat(\"üìä Creating summary visualizations...\\n\")\n",
    "\n",
    "# Function to create summary plots\n",
    "create_summary_plots <- function(data, component_name) {\n",
    "  if (is.null(data) || nrow(data) == 0) {\n",
    "    cat(\"‚ö†Ô∏è No data available for\", component_name, \"plots\\n\")\n",
    "    return(NULL)\n",
    "  }\n",
    "  \n",
    "  cat(\"\\nüìà Creating plots for\", component_name, \"...\\n\")\n",
    "  \n",
    "  # Get numeric columns (excluding ID columns)\n",
    "  exclude_cols <- c(\"code_muni\", \"year\", \"abbrev_state\", \"name_muni\")\n",
    "  numeric_cols <- names(data)[sapply(data, is.numeric) & \n",
    "                              !names(data) %in% exclude_cols]\n",
    "  \n",
    "  if (length(numeric_cols) == 0) {\n",
    "    cat(\"   ‚ö†Ô∏è No numeric variables found\\n\")\n",
    "    return(NULL)\n",
    "  }\n",
    "  \n",
    "  # Take first few numeric variables for plotting\n",
    "  plot_vars <- head(numeric_cols, 4)\n",
    "  \n",
    "  plots <- list()\n",
    "  \n",
    "  for (var in plot_vars) {\n",
    "    tryCatch({\n",
    "      # Remove missing values\n",
    "      plot_data <- data[!is.na(data[[var]]), ]\n",
    "      \n",
    "      if (nrow(plot_data) > 0) {\n",
    "        # Histogram\n",
    "        p <- ggplot(plot_data, aes_string(x = var)) +\n",
    "          geom_histogram(bins = 20, fill = \"steelblue\", alpha = 0.7) +\n",
    "          labs(title = paste(\"Distribution of\", var),\n",
    "               subtitle = paste(component_name, \"component\"),\n",
    "               x = var, y = \"Count\") +\n",
    "          theme_minimal()\n",
    "        \n",
    "        print(p)\n",
    "        plots[[var]] <- p\n",
    "        \n",
    "        # Basic statistics\n",
    "        cat(\"   üìä\", var, \":\", \n",
    "            \"Mean =\", round(mean(plot_data[[var]], na.rm = TRUE), 2),\n",
    "            \"| Median =\", round(median(plot_data[[var]], na.rm = TRUE), 2),\n",
    "            \"| Range =\", round(min(plot_data[[var]], na.rm = TRUE), 2), \"-\",\n",
    "            round(max(plot_data[[var]], na.rm = TRUE), 2), \"\\n\")\n",
    "      }\n",
    "    }, error = function(e) {\n",
    "      cat(\"   ‚ö†Ô∏è Could not plot\", var, \":\", e$message, \"\\n\")\n",
    "    })\n",
    "  }\n",
    "  \n",
    "  return(plots)\n",
    "}\n",
    "\n",
    "# Create plots for each component with key indicators\n",
    "summary_plots <- list()\n",
    "\n",
    "for (component in names(key_indicators)) {\n",
    "  plots <- create_summary_plots(key_indicators[[component]], component)\n",
    "  if (!is.null(plots)) {\n",
    "    summary_plots[[component]] <- plots\n",
    "  }\n",
    "}\n",
    "\n",
    "cat(\"\\n‚úÖ Summary visualizations complete!\\n\")"
   ]
